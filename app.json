{
  "expo": {
    "name": "nademi-tasklist",
    "slug": "nademi-tasklist",
    "version": "3.4.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "bundleIdentifier": "nademi.task.list",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "dark",
      "package": "nademi.task.list",
      "versionCode": 58
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/favicon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "description": "A simple App for storing Tasks",
    "githubUrl": "https://github.com/nesho84/nademi-tasklist",
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#343a40"
    },
    "extra": {
      "eas": {
        "projectId": "3688c8ff-83ef-4da3-94ab-680e4f254f38"
      }
    }
  }
}